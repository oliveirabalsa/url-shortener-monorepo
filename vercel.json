{
  "version": 2,
  "builds": [
    {
      "src": "apps/api/dist/main.js",
      "use": "@vercel/node"
    },
    {
      "src": "apps/client/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/api/dist/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "apps/client/dist/index.html"
    }
  ]
}
