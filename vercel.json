{
  "builds": [
    {
      "src": "apps/api/dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/api/dist/main.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/(.*)",
      "dest": "apps/api/dist/main.js",
      "methods": ["GET"]
    }
  ]
}
